local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
    Title = "CrackHub GUI",
    Style = 1,
    SizeX = 300,
    SizeY = 350,
    Theme = "Dark",
    ColorOverrides = {
        MainFrame = Color3.fromRGB(0,10,0)
    }
})

local Y = X.New({
    Title = "Main",
        X.Banner{
        Text = "Please go to Universal and disable Noclip if you want to stop floating."
        }
})

local M = X.New({
    Title = "Universal"
})


local Z = X.New({
    Title = "Info"
})

 local C = M.Button({
    Text = "Reset",
    Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end,
       Menu = {
           Info = function(self)
           X.Banner({
               Text = "Kills you"
           })
      end
       }
})

local L = Y.Button({
  Text = "Funky Filters",
    Callback = function()
      loadstring(game:HttpGet("https://pastebin.com/raw/6AFn8F8x"))()
    end
})

local O = Y.Button({
    Text = "R6 Nugget",
        Callback = function()
            local la = game.Players.LocalPlayer.Character["Left Arm"]
            local ra = game.Players.LocalPlayer.Character["Right Arm"]
            local ll = game.Players.LocalPlayer.Character["Left Leg"]
            local rl = game.Players.LocalPlayer.Character["Right Leg"]
            local hm = game.Players.LocalPlayer.Character.Head.Mesh
            for i,hats in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if hats:IsA("Accessory") then
            la:Destroy()
            ra:Destroy()
            ll:Destroy()
            rl:Destroy()
            hm:Destroy()
            hats:Destroy()
            end
            end
end
})

local P = Y.Button({
    Text = "R15 Nugget",
        Callback = function()
        local lla = game.Players.LocalPlayer.Character.LeftLowerArm
    local lua = game.Players.LocalPlayer.Character.LeftUpperArm
        local lll = game.Players.LocalPlayer.Character.LeftLowerLeg
            local lul = game.Players.LocalPlayer.Character.LeftUpperLeg
                local lh = game.Players.LocalPlayer.Character.LeftHand
                    local lf = game.Players.LocalPlayer.Character.LeftFoot
                        local rla = game.Players.LocalPlayer.Character.RightLowerArm
                    local rua = game.Players.LocalPlayer.Character.RightUpperArm
                local rll = game.Players.LocalPlayer.Character.RightLowerLeg
            local rul = game.Players.LocalPlayer.Character.RightUpperLeg
        local rh = game.Players.LocalPlayer.Character.RightHand
    local rf = game.Players.LocalPlayer.Character.RightFoot
local hm = game.Players.LocalPlayer.Character.Head.Head
for i,hats in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
    if hats:IsA("Accessory") then
lla:Destroy()
    lua:Destroy()
        lla:Destroy()
            lul:Destroy()
                lh:Destroy()
                    lf:Destroy()
                        rll:Destroy()
                    rul:Destroy()
                rua:Destroy()
            rh:Destroy()
        rf:Destroy()
    hm:Destroy()
hats:Destroy()
    end
end
        end
})

local F = Y.Button({
    Text = "R6 No Legs",
 Callback = function()
game.Players.LocalPlayer.Character["Left Leg"]:Destroy()
game.Players.LocalPlayer.Character["Right Leg"]:Destroy() 
 end
})

local invislegs = Y.Button({
    Text = "Invis Legs (R6 ONLY)",
        Callback = function()
            game.Players.LocalPlayer.Character["Left Leg"]:Destroy()
            game.Players.LocalPlayer.Character["Right Leg"]:Destroy()
            game.Players.LocalPlayer.Character.Humanoid.HipHeight = 2
        end
})

local Q = Y.Button({
    Text = "R15 No Legs",
     Callback = function()
        game.Players.LocalPlayer.Character.LeftLowerLeg:Destroy()
        game.Players.LocalPlayer.Character.RightLowerLeg:Destroy()
        game.Players.LocalPlayer.Character.LeftUpperLeg:Destroy()
        game.Players.LocalPlayer.Character.RightUpperLeg:Destroy()
        game.Players.LocalPlayer.Character.LeftFoot:Destroy()
        game.Players.LocalPlayer.Character.RightFoot:Destroy()
     end
        
})

 local walkspeedslider = M.Slider({
     Text = "Set WalkSpeed",
        Callback = function(Value)
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
        end,
    Min = 1,
    Max = 180,
    Def = 16
 })
 
  local jmp = M.Slider({
      Text = "Set JumpPower",
        Callback = function(Value)
         game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
        end,
    Min = 0,
    Max = 1000,
    Def = 70
  })
  
  local umt = Y.Button({
      Text = "Universal Multi-Tool",
       Callback = function()
            loadstring(game:HttpGet("https://pastebin.com/raw/1vE2PRkT"))()
       end,
       Menu = {
            IMPORTANT = function(self)
           X.Banner({
               Text = "I do not take all the credit for this one, many people helped with this."
           })
       end    
       }
       
  })
  
  local av = Y.Button({
      Text = "A V.2",
      Callback = function()
            loadstring(game:HttpGet("https://pastebin.com/raw/JDMRACum"))()
    end
  })

 local noface = Y.Button({
     Text = "No Face",
     Callback = function()
        game:GetService("Players").LocalPlayer.Character.Head.face:Destroy()
     end
 })
 
 local noclip = M.Toggle({
     Text = "Noclip",
     Enabled = true,
     Callback = function()
     game:GetService("RunService").RenderStepped:connect(function()
	if Enabled then
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
	return
 end
 end)
    Enabled = not Enabled
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(2)
	return
end
 })
 
 

 local V = Z.Button({
    Text = "Credits",
 Callback = function()
   X.Banner
   {
       Text = "Made by Funny Killingtine#2547/killing27564"
   }
        end            
        })
