 }) 
 
 local noclip = M.Toggle({
     Text = "Noclip",
     Enabled = true,
     Callback = function()
     game:GetService("RunService").RenderStepped:connect(function()
	if Enabled then
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
	return
 end
 end)
    Enabled = not Enabled
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(2)
	return
end
 })
 

 local V = Z.Button({
    Text = "Credits",
 Callback = function()
   X.Banner
   {
       Text = "Made by Goku Black#8597/Proxo_101, UI used is MaterialLua"
   }
        end            
        })
    
    local pulser = games.Button({
        Text = "Miners Haven Auto Pulser",
        Callback = function()
        game:GetService("RunService").RenderStepped:connect(function()
    if game.Players.LocalPlayer.PlayerGui.GUI.HUDBottom.Limit.Limit.Text == "Ore Limit: 250/250" then
        game:GetService("ReplicatedStorage").Pulse:FireServer()
    end
    end)
        end,
    Menu = {
        Important = function(self)
            X.Banner({
                Text = "Set Ore Limit to 250 before using."
            })
        end
       }
    })
    
local G = M.ColorPicker({
    Text = "BackGround Colour",
    Default = Default_Color,
    Callback = function(Value)
        game:GetService("CoreGui"):FindFirstChild(Gui_Name).MainFrame.ImageColor3 = Color3.fromRGB(Value.R * 255, Value.G * 255, Value.B * 255)
    end,
    Menu = {
        Information = function(self)
            X.Banner({
                Text = "This changes the color of the GUI BackGround."
            })
        end
    }
})
